<script type="text/javascript" src="http://7xthla.com1.z0.glb.clouddn.com/bus/js/jquery-1.12.0.min.js"></script>
<script type="text/javascript" src="http://7xthla.com1.z0.glb.clouddn.com/bus/js/base.js"></script>
<script>
    var is_show_error = true;
    function refreshLineStatus()
    {
        //清除历史记录
        $("#busnum").html('');
        $(".lineDetail dt i").siblings('b').before("&nbsp;");
        $(".lineDetail dd").each(function(){
            l=$(this).children("p").length;
            if(l==2){
                $(this).find("span:last").html('');
            }

        })

        $.post("api_line_status.php", { lineID: 10000310},
            function(result){
            result = eval("("+result+")");
            if(result.status==1){
                var busnum=0;
                for (var i=0; i<result.data.length; i++){
                    var id = result.data[i]['ID'];
                    var Intime = result.data[i]['InTime'];
                    var BusInfo = result.data[i]['BusInfo'];
                    var num = $("#"+id+" dt b").html();
                    if(BusInfo){
                        var html = result.data[i]['BusInfo']+'&nbsp;&nbsp;&nbsp;&nbsp;'+Intime;
                        $("#"+id+" dd span:last").html(html);
                        if(!$("#"+id+" dd").hasClass("inIdItem")){
                            $("#"+id+" dd").addClass("inIdItem");
			            }
                        $("#"+id+" dt").html('<i class="icomoon">&#xe530;</i><b>'+num+'</b>');
                        busnum++;
                    }else{
                        if($("#"+id+" dd").hasClass("inIdItem")){
                            $("#"+id+" dd").removeClass("inIdItem");
                        }
                        $("#"+id+" dt").html('&nbsp;<b>'+num+'</b>');
                    }
                }
                len = $(".inIdItem").length;
                if(len>0){
                    $("#busnum").html('('+len+'辆正在行驶)');
                }
            }else{
                if(is_show_error){
//                    $('#noticeA').OpenDiv();
//                    $('#BigDiv').on('click',function(){
//                        $('#noticeA').CloseDiv();
//                    });
                    $("#busnum").html('(实时数据失联)');
                    is_show_error = false;
                }
            }
        });

        setTimeout("refreshLineStatus()",10000);
    }
    $(document).ready(function() {


        //查询实时线路信息
        refreshLineStatus();

        var openid = "0JYRi4XzCJqcmTZx5mFZG9w0hcRrMRy0";
        //收藏线路
        $(".star").click(function(){
            var co = $(this).attr('co');
            if(co==0){            //搜藏线路
                co = $(this).attr('co',1);
                $(this).find("i").html("&#xf005;");
                var action = 'add';
                $("#toast .weui_toast_content").html('已收藏');
            }else{            //取消搜藏线路
                co = $(this).attr('co',0);
                $(this).find("i").html("&#xf006");
                var action = 'delete';
                $("#toast .weui_toast_content").html('已取消');
            }

            var lineID = $(this).attr('lineID');

            $.post("api_collect_line.php", { lineID: lineID, openid: openid, action: action},function(result){
                console.log(result);
                $('#toast').show();
                setTimeout(function () {
                    $('#toast').hide();
                }, 2000);
            });
        });

    });
</script>

<script type="text/javascript">
    wx.config({
        debug: false,
        appId: 'wx418b1e6e20529814',
        timestamp: 1524712279,
        nonceStr: 'Ymmss15kpd7sNPm4',
        signature: 'cb9aae77e92eaed1cf79ec8f2b0c61405de293a7',
        jsApiList: [
            'checkJsApi',
            'onMenuShareTimeline',
            'onMenuShareAppMessage',
            'getLocation'
        ]
    });

    wx.ready(function(){
        wx.getLocation({
            type: 'wgs84', // 默认为wgs84的gps坐标，如果要返回直接给openLocation用的火星坐标，可传入'gcj02'
            success: function (res) {console.log(res);
                var latitude = res.latitude; // 纬度，浮点数，范围为90 ~ -90
                var longitude = res.longitude; // 经度，浮点数，范围为180 ~ -180。
                var speed = res.speed; // 速度，以米/每秒计
                var accuracy = res.accuracy; // 位置精度

                //查询用户附近站点
                $.post("api_line_nearby.php", { latitude: latitude, longitude: longitude ,lineID:10000310},function(result){
                    result = eval("("+result+")");
                    if(result.status==1){
                        var id = result.data.station_id;
                        num = $("#"+id+" dt b").html();
                        $("#"+id+" dt").html('<i class="icomoon">&#xf007;</i> <b class="nearer ">'+num+'</b>');
                        $("#"+id+" dd b:last").after('<span style="color:red">约'+result.data.distance+'米</span>')
                    }
                });
            }
        });

        wx.onMenuShareTimeline({
            title: '苏州公交87路实时查询', // 分享标题
            link: window.location.href, // 分享链接
            imgUrl: window.location.origin+'/css/share.jpg', // 分享图标
            success: function () {
                // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });

        wx.onMenuShareAppMessage({
            title: '苏州公交87路实时查询', // 分享标题
            desc: '苏州公交官方微信平台，坐公交就得用这个！', // 分享描述
            link: window.location.href, // 分享链接
            imgUrl: window.location.origin+'/css/share.jpg', // 分享图标
            type: '', // 分享类型,music、video或link，不填默认为link
            dataUrl: '', // 如果type是music或video，则要提供数据链接，默认为空
            success: function () {
                // 用户确认分享后执行的回调函数
            },
            cancel: function () {
                // 用户取消分享后执行的回调函数
            }
        });
    });
</script>
